@using MyFace.Controllers
@model MyFace.Models.View.UsersViewModel

@{
    Layout = "_Layout";
}

@{

    foreach (var user in@Model.Users)
    {
        <a class="nav-link text-dark" asp-controller="Users" asp-action="UserPage" asp-route-id="@user.Id">
            <b><p>@user.Username</p></b>
        </a>
        
    }
}

